.about-home__wrapper
    .container
        .columns.is-veriable.is-gapless
            .column.is-6
                .about-home__text-wrapper
                    %h3 About Me
                    %p
                        My name is Nicole Redacted and I'm currently based in New York. My main areas of focus are Graphic and UX/UI Design and Front End Develop. 
                    .btn__wrapper
                        = link_to "Find out more", "/about", :class => "btn"
            .column.is-6

.services-home__wrapper
    .container
        .columns
            .column.is-6
                .services-home__title-wrapper
                    %h2 Creating unique experiences
                    .btn__wrapper.is-hidden-mobile
                        = link_to "View All Skills", "http://www.google.com", :class => "btn"
            .column.is-6
                .services-home__list-wrapper
                    .columns.is-mobile.is-centered
