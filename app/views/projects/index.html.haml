.hero__wrapper
    .container
        .hero__text
            .h2
                .columns.is-variable.is-gapless.is-multiline
                    .column.is-narrow-desktop.is-12-touch.hero__wave-wrapper
                        = image_tag 'wave.svg', alt: 'wave', class: 'wave-wrapper__wave-img'
                    .column.is-narrow-desktop.is-12-touch.hero__small-text
                        %p Hi, my name is Nicole Redacted and I'm a...
            %h1.hero__big-text
                #hero__variable-text
                %span
                    Designer &
                %br
                    Front End Developer

.projects-home__wrapper
    .container
        %h3 Projects
        .columns.is-variable.is-5
            .column.is-4-desktop.is-6-tablet
                = link_to "http://www.google.com", :class => "project-button", :target => "blank" do
                    .projects-home__cover-wrapper
                        = image_tag 'intimates-preview.png', alt: 'Intimates'
                        .projects-home__arrow.purple
                            = inline_svg('arrow.svg')
                    .project-home__project-summary
                        %h4 Project title
                        %p.projects-home__project-description
                            Website redesign for Island Finance, the leading company in personal loans in Puerto Rico. 
            .column.is-4-desktop.is-6-tablet
                = link_to "http://www.google.com", :class => "project-button", :target => "blank" do
                    .projects-home__cover-wrapper
                        = inline_svg('project-cover.svg')
                        .projects-home__arrow.green 
                            = inline_svg('arrow.svg')
                    .project-home__project-summary
                        %h4 Project title
                        %p.projects-home__project-description
                            Website redesign for Island Finance, the leading company in personal loans in Puerto Rico. 
            .column.is-4-desktop.is-hidden-tablet-only	
                = link_to "http://www.google.com", :class => "project-button", :target => "blank" do
                    .projects-home__cover-wrapper
                        = inline_svg('project-cover.svg')
                        .projects-home__arrow.yellow
                            = inline_svg('arrow.svg')
                    .project-home__project-summary
                        %h4 Project title
                        %p.projects-home__project-description
                            Website redesign for Island Finance, the leading company in personal loans in Puerto Rico.
        .btn__wrapper
            = link_to "View All Projects", "http://www.google.com", :class => "btn"


